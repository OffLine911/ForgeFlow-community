[
  {
    "id": "daily-reminder",
    "name": "Daily Reminder",
    "description": "Get a notification reminder every day at a scheduled time",
    "icon": "üîî",
    "category": "Productivity",
    "author": "OffLine911",
    "nodes": [
      {
        "type": "trigger_schedule",
        "position": { "x": 100, "y": 150 },
        "data": {
          "cron": "0 9 * * *"
        }
      },
      {
        "type": "action_notification",
        "position": { "x": 350, "y": 150 },
        "data": {
          "title": "Daily Reminder",
          "message": "Don't forget to check your tasks for today!"
        }
      }
    ],
    "connections": [
      {
        "sourceIndex": 0,
        "sourcePort": "out",
        "targetIndex": 1,
        "targetPort": "in"
      }
    ]
  },
  {
    "id": "file-organizer",
    "name": "Smart File Organizer",
    "description": "Automatically organize files in a folder by their extension",
    "icon": "üìÅ",
    "category": "Automation",
    "author": "OffLine911",
    "nodes": [
      {
        "type": "trigger_file_watch",
        "position": { "x": 100, "y": 150 },
        "data": {
          "path": "C:\\Users\\YourName\\Downloads",
          "events": "create"
        }
      },
      {
        "type": "action_file_list",
        "position": { "x": 350, "y": 150 },
        "data": {
          "path": "C:\\Users\\YourName\\Downloads",
          "pattern": "*.*",
          "include": "files"
        }
      },
      {
        "type": "loop_foreach",
        "position": { "x": 600, "y": 150 },
        "data": {
          "array": "{{output}}",
          "itemVar": "file",
          "indexVar": "i"
        }
      },
      {
        "type": "action_script",
        "position": { "x": 850, "y": 100 },
        "data": {
          "command": "powershell",
          "args": "-Command \"$ext = [System.IO.Path]::GetExtension('{{file.path}}').TrimStart('.'); $dest = 'C:\\Users\\YourName\\Downloads\\' + $ext.ToUpper(); if(!(Test-Path $dest)){New-Item -ItemType Directory -Path $dest}; Move-Item '{{file.path}}' $dest -Force\""
        }
      },
      {
        "type": "action_notification",
        "position": { "x": 600, "y": 300 },
        "data": {
          "title": "Files Organized",
          "message": "Your downloads folder has been organized!"
        }
      }
    ],
    "connections": [
      { "sourceIndex": 0, "sourcePort": "out", "targetIndex": 1, "targetPort": "in" },
      { "sourceIndex": 1, "sourcePort": "out", "targetIndex": 2, "targetPort": "in" },
      { "sourceIndex": 2, "sourcePort": "loop", "targetIndex": 3, "targetPort": "in" },
      { "sourceIndex": 2, "sourcePort": "done", "targetIndex": 4, "targetPort": "in" }
    ]
  },
  {
    "id": "api-health-check",
    "name": "API Health Monitor",
    "description": "Check multiple API endpoints and notify if any are down",
    "icon": "üåê",
    "category": "DevOps",
    "author": "OffLine911",
    "nodes": [
      {
        "type": "trigger_schedule",
        "position": { "x": 100, "y": 150 },
        "data": {
          "cron": "*/5 * * * *"
        }
      },
      {
        "type": "action_http",
        "position": { "x": 350, "y": 100 },
        "data": {
          "method": "GET",
          "url": "https://api.github.com/status",
          "headers": "{}"
        }
      },
      {
        "type": "condition_if",
        "position": { "x": 600, "y": 100 },
        "data": {
          "condition": "{{output.status}} !== 200"
        }
      },
      {
        "type": "action_notification",
        "position": { "x": 850, "y": 50 },
        "data": {
          "title": "‚ö†Ô∏è API Alert",
          "message": "API health check failed! Status: {{output.status}}"
        }
      },
      {
        "type": "action_log",
        "position": { "x": 850, "y": 180 },
        "data": {
          "message": "API health check passed",
          "level": "info"
        }
      }
    ],
    "connections": [
      { "sourceIndex": 0, "sourcePort": "out", "targetIndex": 1, "targetPort": "in" },
      { "sourceIndex": 1, "sourcePort": "out", "targetIndex": 2, "targetPort": "in" },
      { "sourceIndex": 2, "sourcePort": "true", "targetIndex": 3, "targetPort": "in" },
      { "sourceIndex": 2, "sourcePort": "false", "targetIndex": 4, "targetPort": "in" }
    ]
  },
  {
    "id": "ai-summarizer",
    "name": "AI Text Summarizer",
    "description": "Summarize text content using AI (requires Ollama or API key)",
    "icon": "ü§ñ",
    "category": "AI",
    "author": "OffLine911",
    "nodes": [
      {
        "type": "trigger_manual",
        "position": { "x": 100, "y": 150 },
        "data": {}
      },
      {
        "type": "action_file",
        "position": { "x": 350, "y": 150 },
        "data": {
          "mode": "read",
          "path": "C:\\path\\to\\your\\document.txt"
        }
      },
      {
        "type": "action_ai",
        "position": { "x": 600, "y": 150 },
        "data": {
          "provider": "ollama",
          "model": "llama3.2",
          "systemPrompt": "You are a helpful assistant that creates concise summaries.",
          "prompt": "Please summarize the following text in 3-5 bullet points:\n\n{{output}}",
          "temperature": 0.7
        }
      },
      {
        "type": "action_file",
        "position": { "x": 850, "y": 150 },
        "data": {
          "mode": "write",
          "path": "C:\\path\\to\\summary.txt",
          "content": "{{output}}"
        }
      },
      {
        "type": "action_notification",
        "position": { "x": 1100, "y": 150 },
        "data": {
          "title": "Summary Complete",
          "message": "Your document has been summarized!"
        }
      }
    ],
    "connections": [
      { "sourceIndex": 0, "sourcePort": "out", "targetIndex": 1, "targetPort": "in" },
      { "sourceIndex": 1, "sourcePort": "out", "targetIndex": 2, "targetPort": "in" },
      { "sourceIndex": 2, "sourcePort": "out", "targetIndex": 3, "targetPort": "in" },
      { "sourceIndex": 3, "sourcePort": "out", "targetIndex": 4, "targetPort": "in" }
    ]
  },
  {
    "id": "csv-to-json",
    "name": "CSV to JSON Converter",
    "description": "Convert CSV files to JSON format with one click",
    "icon": "üìä",
    "category": "Data",
    "author": "OffLine911",
    "nodes": [
      {
        "type": "trigger_manual",
        "position": { "x": 100, "y": 150 },
        "data": {}
      },
      {
        "type": "action_file",
        "position": { "x": 350, "y": 150 },
        "data": {
          "mode": "read",
          "path": "C:\\data\\input.csv"
        }
      },
      {
        "type": "action_csv_parse",
        "position": { "x": 600, "y": 150 },
        "data": {
          "csv": "{{output}}",
          "delimiter": ",",
          "headers": true
        }
      },
      {
        "type": "utility_json_stringify",
        "position": { "x": 850, "y": 150 },
        "data": {
          "input": "{{output}}",
          "pretty": true
        }
      },
      {
        "type": "action_file",
        "position": { "x": 1100, "y": 150 },
        "data": {
          "mode": "write",
          "path": "C:\\data\\output.json",
          "content": "{{output}}"
        }
      },
      {
        "type": "action_notification",
        "position": { "x": 1350, "y": 150 },
        "data": {
          "title": "Conversion Complete",
          "message": "CSV has been converted to JSON!"
        }
      }
    ],
    "connections": [
      { "sourceIndex": 0, "sourcePort": "out", "targetIndex": 1, "targetPort": "in" },
      { "sourceIndex": 1, "sourcePort": "out", "targetIndex": 2, "targetPort": "in" },
      { "sourceIndex": 2, "sourcePort": "out", "targetIndex": 3, "targetPort": "in" },
      { "sourceIndex": 3, "sourcePort": "out", "targetIndex": 4, "targetPort": "in" },
      { "sourceIndex": 4, "sourcePort": "out", "targetIndex": 5, "targetPort": "in" }
    ]
  },
  {
    "id": "backup-workflow",
    "name": "Automated Backup",
    "description": "Compress and backup important folders on schedule",
    "icon": "üíæ",
    "category": "Automation",
    "author": "OffLine911",
    "nodes": [
      {
        "type": "trigger_schedule",
        "position": { "x": 100, "y": 150 },
        "data": {
          "cron": "0 2 * * 0"
        }
      },
      {
        "type": "action_date",
        "position": { "x": 350, "y": 150 },
        "data": {
          "operation": "now",
          "format": "YYYY-MM-DD"
        }
      },
      {
        "type": "action_zip_compress",
        "position": { "x": 600, "y": 150 },
        "data": {
          "sources": "C:\\ImportantFiles",
          "zipPath": "D:\\Backups\\backup-{{output}}.zip"
        }
      },
      {
        "type": "action_notification",
        "position": { "x": 850, "y": 150 },
        "data": {
          "title": "Backup Complete",
          "message": "Weekly backup created successfully!"
        }
      }
    ],
    "connections": [
      { "sourceIndex": 0, "sourcePort": "out", "targetIndex": 1, "targetPort": "in" },
      { "sourceIndex": 1, "sourcePort": "out", "targetIndex": 2, "targetPort": "in" },
      { "sourceIndex": 2, "sourcePort": "out", "targetIndex": 3, "targetPort": "in" }
    ]
  },
  {
    "id": "webhook-processor",
    "name": "Webhook Processor",
    "description": "Receive and process incoming webhooks with logging",
    "icon": "üîó",
    "category": "Integration",
    "author": "OffLine911",
    "nodes": [
      {
        "type": "trigger_webhook",
        "position": { "x": 100, "y": 150 },
        "data": {
          "path": "/incoming",
          "method": "POST"
        }
      },
      {
        "type": "utility_json_parse",
        "position": { "x": 350, "y": 150 },
        "data": {
          "input": "{{output}}"
        }
      },
      {
        "type": "action_log",
        "position": { "x": 600, "y": 100 },
        "data": {
          "message": "Webhook received: {{output}}",
