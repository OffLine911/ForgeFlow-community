      { "sourceIndex": 1, "sourcePort": "out", "targetIndex": 2, "targetPort": "in" },
      { "sourceIndex": 1, "sourcePort": "out", "targetIndex": 3, "targetPort": "in" },
      { "sourceIndex": 2, "sourcePort": "out", "targetIndex": 4, "targetPort": "in" }
    ]
  },
