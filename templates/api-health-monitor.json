{
  "id": "api-health-monitor",
  "name": "API Health Monitor",
  "description": "Monitor multiple API endpoints and get alerts when they're down",
  "icon": "activity",
  "category": "DevOps",
  "nodes": [
    {
      "type": "trigger_schedule",
      "position": { "x": 100, "y": 200 },
      "data": {
        "cron": "*/5 * * * *"
      }
    },
    {
      "type": "action_http",
      "position": { "x": 350, "y": 200 },
      "data": {
        "method": "GET",
        "url": "https://api.github.com/status"
      }
    },
    {
      "type": "action_notification",
      "position": { "x": 600, "y": 200 },
      "data": {
        "title": "API Health Check",
        "message": "Status: {{output.status}}"
      }
    },
    {
      "type": "action_log",
      "position": { "x": 850, "y": 200 },
      "data": {
        "message": "Health check completed at {{timestamp}}"
      }
    }
  ],
  "connections": [
    {
      "sourceIndex": 0,
      "sourcePort": "out",
      "targetIndex": 1,
      "targetPort": "in"
    },
    {
      "sourceIndex": 1,
      "sourcePort": "out",
      "targetIndex": 2,
      "targetPort": "in"
    },
    {
      "sourceIndex": 2,
      "sourcePort": "out",
      "targetIndex": 3,
      "targetPort": "in"
    }
  ]
}
