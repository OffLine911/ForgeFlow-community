{
  "id": "csv-to-json-converter",
  "name": "CSV to JSON Converter",
  "description": "Convert CSV files to JSON format with data validation and formatting",
  "icon": "file-text",
  "category": "Data",
  "nodes": [
    {
      "type": "trigger_file_watch",
      "position": { "x": 100, "y": 200 },
      "data": {
        "path": "C:\\Data\\Input",
        "pattern": "*.csv"
      }
    },
    {
      "type": "action_file",
      "position": { "x": 350, "y": 200 },
      "data": {
        "operation": "read",
        "path": "{{filePath}}"
      }
    },
    {
      "type": "action_csv_parse",
      "position": { "x": 600, "y": 200 },
      "data": {
        "input": "{{output}}"
      }
    },
    {
      "type": "utility_json_stringify",
      "position": { "x": 850, "y": 200 },
      "data": {
        "input": "{{output}}"
      }
    },
    {
      "type": "action_file",
      "position": { "x": 1100, "y": 200 },
      "data": {
        "operation": "write",
        "path": "C:\\Data\\Output\\output.json",
        "content": "{{output}}"
      }
    },
    {
      "type": "action_notification",
      "position": { "x": 1350, "y": 200 },
      "data": {
        "title": "Conversion Complete",
        "message": "CSV converted to JSON successfully"
      }
    }
  ],
  "connections": [
    {
      "sourceIndex": 0,
      "sourcePort": "out",
      "targetIndex": 1,
      "targetPort": "in"
    },
    {
      "sourceIndex": 1,
      "sourcePort": "out",
      "targetIndex": 2,
      "targetPort": "in"
    },
    {
      "sourceIndex": 2,
      "sourcePort": "out",
      "targetIndex": 3,
      "targetPort": "in"
    },
    {
      "sourceIndex": 3,
      "sourcePort": "out",
      "targetIndex": 4,
      "targetPort": "in"
    },
    {
      "sourceIndex": 4,
      "sourcePort": "out",
      "targetIndex": 5,
      "targetPort": "in"
    }
  ]
}
