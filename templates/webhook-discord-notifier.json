{
  "id": "webhook-discord-notifier",
  "name": "Webhook to Discord Notifier",
  "description": "Receive webhooks and send formatted notifications to Discord channels",
  "icon": "bell",
  "category": "Integration",
  "nodes": [
    {
      "type": "trigger_webhook",
      "position": { "x": 100, "y": 200 },
      "data": {
        "path": "/notify",
        "method": "POST"
      }
    },
    {
      "type": "action_http",
      "position": { "x": 350, "y": 200 },
      "data": {
        "method": "POST",
        "url": "https://discord.com/api/webhooks/YOUR_WEBHOOK_URL",
        "body": "{\"content\": \"{{body.message}}\"}"
      }
    },
    {
      "type": "action_log",
      "position": { "x": 600, "y": 200 },
      "data": {
        "message": "Sent to Discord: {{body.message}}"
      }
    }
  ],
  "connections": [
    {
      "sourceIndex": 0,
      "sourcePort": "out",
      "targetIndex": 1,
      "targetPort": "in"
    },
    {
      "sourceIndex": 1,
      "sourcePort": "out",
      "targetIndex": 2,
      "targetPort": "in"
    }
  ]
}
