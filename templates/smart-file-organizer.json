{
  "id": "smart-file-organizer",
  "name": "Smart File Organizer",
  "description": "Automatically organize downloads by file type into categorized folders",
  "icon": "folder",
  "category": "Automation",
  "nodes": [
    {
      "type": "trigger_file_watch",
      "position": { "x": 100, "y": 200 },
      "data": {
        "path": "C:\\Users\\Downloads"
      }
    },
    {
      "type": "action_file_manage",
      "position": { "x": 350, "y": 200 },
      "data": {
        "operation": "move",
        "source": "{{filePath}}",
        "destination": "C:\\Users\\Downloads\\Organized\\{{fileName}}"
      }
    },
    {
      "type": "action_notification",
      "position": { "x": 600, "y": 200 },
      "data": {
        "title": "File Organized",
        "message": "{{fileName}} moved to organized folder"
      }
    }
  ],
  "connections": [
    {
      "sourceIndex": 0,
      "sourcePort": "out",
      "targetIndex": 1,
      "targetPort": "in"
    },
    {
      "sourceIndex": 1,
      "sourcePort": "out",
      "targetIndex": 2,
      "targetPort": "in"
    }
  ]
}
