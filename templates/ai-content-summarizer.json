{
  "id": "ai-content-summarizer",
  "name": "AI Content Summarizer",
  "description": "Fetch web content and generate AI-powered summaries with key insights",
  "icon": "ü§ñ",
  "category": "AI",
  "nodes": [
    {
      "type": "trigger_webhook",
      "position": { "x": 100, "y": 200 },
      "data": {
        "path": "/summarize",
        "method": "POST"
      }
    },
    {
      "type": "action_http",
      "position": { "x": 350, "y": 200 },
      "data": {
        "method": "GET",
        "url": "{{body.url}}",
        "headers": {
          "User-Agent": "Mozilla/5.0"
        }
      }
    },
    {
      "type": "action_text",
      "position": { "x": 600, "y": 200 },
      "data": {
        "operation": "extract",
        "pattern": "<body[^>]*>([\\s\\S]*?)</body>",
        "input": "{{output}}"
      }
    },
    {
      "type": "action_text",
      "position": { "x": 850, "y": 200 },
      "data": {
        "operation": "replace",
        "pattern": "<[^>]+>",
        "replacement": "",
        "input": "{{output}}"
      }
    },
    {
      "type": "action_ai",
      "position": { "x": 1100, "y": 200 },
      "data": {
        "provider": "openai",
        "model": "gpt-4",
        "prompt": "Summarize the following content in 3-5 bullet points with key insights:\n\n{{output}}",
        "maxTokens": 500
      }
    },
    {
      "type": "action_notification",
      "position": { "x": 1350, "y": 150 },
      "data": {
        "title": "Summary Ready üìù",
        "message": "AI summary generated successfully"
      }
    },
    {
      "type": "action_file",
      "position": { "x": 1350, "y": 250 },
      "data": {
        "operation": "write",
        "path": "C:\\Summaries\\summary-{{timestamp}}.txt",
        "content": "URL: {{body.url}}\n\nSummary:\n{{output}}"
      }
    }
  ],
  "connections": [
    {
      "sourceIndex": 0,
      "sourcePort": "out",
      "targetIndex": 1,
      "targetPort": "in"
    },
    {
      "sourceIndex": 1,
      "sourcePort": "out",
      "targetIndex": 2,
      "targetPort": "in"
    },
    {
      "sourceIndex": 2,
      "sourcePort": "out",
      "targetIndex": 3,
      "targetPort": "in"
    },
    {
      "sourceIndex": 3,
      "sourcePort": "out",
      "targetIndex": 4,
      "targetPort": "in"
    },
    {
      "sourceIndex": 4,
      "sourcePort": "out",
      "targetIndex": 5,
      "targetPort": "in"
    },
    {
      "sourceIndex": 4,
      "sourcePort": "out",
      "targetIndex": 6,
      "targetPort": "in"
    }
  ]
}
